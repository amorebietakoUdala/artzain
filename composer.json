{
    "name": "amorebietakoudala/zain",
    "license": "MIT",
    "type": "project",
    "description": "Artzain - Control recurring email messages",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "amorebietakoudala/user-bundle": "^1.1",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "jms/serializer-bundle": "^3.5",
        "php-imap/php-imap": "3.0.6",
        "secit-pl/imap-bundle": "^1.3",
        "sensio/framework-extra-bundle": "^5.4",
        "symfony/console": "5.*",
        "symfony/flex": "^1.4",
        "symfony/framework-bundle": "5.*",
        "symfony/yaml": "5.*",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/translation": "5.*",
        "symfony/asset": "5.*",
        "symfony/dotenv": "5.*",
        "symfony/security-csrf": "5.*",
        "symfony/security-bundle": "5.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/proxy-manager-bridge": "5.*",
        "symfony/validator": "5.*",
        "symfony/twig-bundle": "5.*",
        "doctrine/annotations": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/property-access": "5.*",
        "symfony/property-info": "5.*",
        "symfony/serializer": "5.*",
        "symfony/runtime": "5.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "hautelook/alice-bundle": "^2.0",
        "nelmio/alice": "^3.1",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/var-dumper": "^4.0",
        "symfony/web-profiler-bundle": "5.*",
        "symfony/stopwatch": "^5.1",
        "symfony/debug-bundle": "^5.1",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.3",
        "symfony/css-selector": "^5.3"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.*"
        }
    }
}
